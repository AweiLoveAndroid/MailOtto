<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.drakeet.mailotto.demo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".preload.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".preload.NextActivity"/>
        <activity android:name=".preload.TargetActivity"/>
        <activity
            android:name=".lazyload.LazyActivity"
            android:label="@string/title_activity_lazy"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>